{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Trace",
      "Override": {
        "Microsoft.AspNetCore": "Trace",
        "Microsoft.EntityFrameworkCore.Database": "Trace"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log.txt"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "EmailMessagePattern": "<div><p>{TEXT}</p></div>",
  "EmailTitlePattern": "Email Title",
  "Quartz": {
    "TablePrefix": "scheduler.qrtz_"
  }
}
